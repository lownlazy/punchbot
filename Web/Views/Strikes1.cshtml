@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
}

<div role="content">
	<table>
		<thead>
		<th>Name</th>
		<th>Power</th>
		</thead>
		<tbody>
			@foreach(var post in CurrentPage.Children) 
			{
				<tr>
					<td>
						<a href="@post.Url"><h2>@post.Name</h2></a>
					</td>
					<td>
						77.7	
					</td>
				</tr>	
			}
		</tbody>
	</table>
</div>